{
  "manifest_version": 2,
  "icons":{"128":"icon.png"},
  "name": "Twitter Share",
  "description": "Share any web page on twitter.",
  "version": "1.0",
  "background":{
    "scripts":["background.js"],
    "persistent":false
    },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "https://twitter.com/intent/tweet", "tabs"
  ]
}
